<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.neumont.lopez.view.BattleShipMenuView"
            prefHeight="400.0" prefWidth="600.0"
            stylesheets="MainMenuView.css">
    <center>
        <HBox fx:id="hbox" styleClass="hbox">
            <Button fx:id="start" styleClass="start"> </Button>
            <Button fx:id="load" styleClass="load"> </Button>
            <Button fx:id="exit" styleClass="exit"> </Button>
        </HBox>
    </center>

</BorderPane>
